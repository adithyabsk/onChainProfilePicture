pragma solidity 0.8.9;

import "@openzeppelin/contracts/token/ERC721/ERC721.sol";
import "@openzeppelin/contracts/token/ERC721/extensions/ERC721URIStorage.sol";
import "base64-sol/base64.sol";

/// @title ProfilePictureNFT
/// @author Adithya Balaji

contract ProfilePicture is ERC721, ERC721URIStorage {
    uint constant TOKEN_ID = 0;
    string constant IMAGE_URI = "data:application/json;base64,";
    event CreatedProfilePictureNFT(uint indexed tokenId, string tokenURI);

    constructor() ERC721("ProfilePicture", "ProPic")
    {
        _safeMint(msg.sender, TOKEN_ID);
        _setTokenURI(TOKEN_ID, formatTokenURI(IMAGE_URI));
        emit CreatedProfilePictureNFT(TOKEN_ID, IMAGE_URI);
    }

    function _burn(uint256 tokenId) internal override(ERC721, ERC721URIStorage) {
        super._burn(tokenId);
    }

    function tokenURI(uint256 tokenId) public view override(ERC721, ERC721URIStorage) returns (string memory) {
        return super.tokenURI(tokenId);
    }

    function formatTokenURI(string memory imageURI) public pure returns (string memory) {
        return string(
            abi.encodePacked(
                "data:application/json;base64,",
                Base64.encode(
                    bytes(
                        abi.encodePacked(
                            '{"name":"',
                            "Profile Picture NFT",
                            '", "description":"Profile Picture NFT for use everywhere!", "attributes":"", "image":"',imageURI,'"}'
                        )
                    )
                )
            )
        );
    }
}
